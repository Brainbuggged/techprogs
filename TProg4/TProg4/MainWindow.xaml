            <TextBox x:Name="box6" HorizontalAlignment="Left" Height="23" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="125"/>
            <TextBox x:Name="box7" HorizontalAlignment="Left" Height="23" Margin="0,44,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="125"/>
